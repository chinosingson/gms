body {
  background: #e7e8ea;
}

/* PROJECT PAGE */

#project-page .project-info {
  padding: 20px 20px;
  background:#3b444c;
  color:#fff;
}

/* temporary style for project titles */
h2.page-header, h2.pane-title {
 color: #01aad5;
 font-family: 'lato',arial,verdana;
 font-weight: 700;
}


/* project info headers*/
#project-page .project-info h4
{
  font-size: 30px;
 color: #01aad5;
 font-family: 'lato',arial,verdana;
 font-weight: 700;

}

.project-info .field-content{
  font-family: 'lato',arial,verdana;
  color:#000;
  font-size:13px;

}

#project-details th.views-field {
  color: #01aad5;
}

#project-page .row{
margin-right:0px !important;
margin-left:0px !important;
}

#project-page .col-sm-12{
margin-right:15px !important;
margin-left:15px !important;
}



/* DASHBOARD */

#panel-bootstrap-region-photos_and_indicators .pane-1{
  border-radius: 5px 5px 0px 0px;
  background: #fff;
  border-bottom: 1px solid #b5b9bc;
}


#panel-bootstrap-region-photos_and_indicators .pane-content {
  padding:10px 10px
  font-size:18px;
  font-family:'lato',arial,sans serif;
}

#panel-bootstrap-region-photos_and_indicators .pane-title {
  color: #3f4448;
  font-family: 'lato',arial,verdana;
  font-weight: 700;
  background: #d1d6da;
  font-size: 30px;
  padding: 10px;
  border-radius: 5px 5px 0px 0px;
  box-shadow: 0px 0px 3px #000;
  border-bottom: 1px solid #eeeeee;
}



#panel-bootstrap-region-photos_and_indicators .field-content{
  color: #000;
  font-size:18px;
  font-family:'lato',arial,sans serif;
}


#panel-bootstrap-region-photos_and_indicators .field-content a{
  font-weight:700;
  font-weight:700;
  color: #4a5559;
  font-size:25px;
}

#panel-bootstrap-region-photos_and_indicators .views-field-title-1 a{
  color:#01aad5;
  font-size:11px;
}

#panel-bootstrap-region-photos_and_indicators .view-project-photos {
  background:#fff;
}


#panel-bootstrap-region-photos_and_indicators .field-content .colorbox{
  background:red;
  width:700px;
}


#panel-bootstrap-region-photos_and_indicators .field-content{
 width:850px;
}


#panel-bootstrap-region-photos_and_indicators .field-content img{
 float:left;
 margin-right:20px;
}

.views-slideshow-cycle-main-frame .views_slideshow_cycle_teaser_section{
  height:280px;
}

#views_slideshow_cycle_teaser_section_project_photos-block_2{
  height:280px !important;
}